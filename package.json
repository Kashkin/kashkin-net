	"browserslist": [
		"> 1%",
		"last 2 versions",
		"Firefox ESR",
		"not ie 10",
		"not op_mini all",
		"not op_mob 12.1",
		"not baidu 7.12",
		"not ie_mob 10",
		"not ie_mob 11",
		"not and_uc 11.8"
	],
	"eslintConfig": {
		"parser": "babel-eslint",
		"plugins": [
			"compat"
		],
		"rules": {
			"compat/compat": "warn"
		}
	},
	"prettier": {
		"trailingComma": "es5",
