		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^5.1.1",
		"eslint": "^6.0.1",
		"eslint-loader": "^2.2.1",
		"eslint-plugin-compat": "^3.3.0",
		"eslint-plugin-vue": "^5.2.3",
		"get-files-in": "^1.2.0",
		"html-critical-webpack-plugin": "^2.1.0",
